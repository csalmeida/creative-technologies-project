{
  "homepage": "/posenet",
  "name": "ctp-installation",
  "author": "Cristiano Almeida",
  "version": "0.1.0",
  "description": "Installation for the Creative Technologies project, using body input to control sound.",
  "private": true,
  "dependencies": {
    "@tensorflow-models/posenet": "^0.2.3",
    "@tensorflow/tfjs": "^0.13.2",
    "ml5": "^0.1.3",
    "p5": "^0.7.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^6.0.0",
    "react-scripts": "^2.1.2",
    "redux": "^4.0.1",
    "styled-components": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "fix-formatting": "prettier --write \"src/**/*.js\"",
    "check-formatting": "prettier --list-different \"src/**/*.js\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
